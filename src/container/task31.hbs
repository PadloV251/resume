<main>
    {{#with navigation}}
        <ul class="list-group list-group-horizontal">
            {{#each links}}
                <div>
                    <a href="{{href}}" class="btn btn-link">{{text}}</a>
                </div>
            {{/each}}
            <a href="{{button.href}}" class="btn btn-primary">{{button.text}}</a>
        </ul>
    {{/with}}
    <hr>
    {{#with header}}
        <h1>{{title}}</h1>
        <br>
        <p>{{description}}</p>
        <br>
        <a href="{{button.link}}" class="btn btn-primary">{{button.text}}</a>
        <hr>
    {{/with}}
    {{#with city}}
        <h2>{{title}}</h2>
        <br>
        <div class="row">
            {{#each cards}}
                <div class="col-6 g-2">
                    <div class="card">
                        <img src="{{picture.src}}" alt="{{picture.alt}}">
                        <div class="card-body">
                            <h5>{{title}}</h5>
                            <p>{{description}}</p>
                            <a href="{{button.href}}" class="btn btn-primary">{{button.text}}</a>
                        </div>
                    </div>
                </div>
            {{/each}}
        </div>
    {{/with}}
    <hr>
    <br>
    {{#with winner}}
        <h2>{{title}}</h2>
        <br>
        {{#with table}}
            <table class="table">
                <thead>
                    <tr>
                        {{#each head}}
                            <th>{{this}}</th>
                        {{/each}}
                    </tr>
                </thead>
                {{#each user}}
                    <tbody>
                        <tr>
                            <th>{{place}}</th>
                            <td>{{name}}</td>
                            <td>{{surname}}</td>
                            <td>{{nickname}}</td>
                        </tr>
                    </tbody>
                {{/each}}
            </table>
        {{/with}}
    {{/with}}
    <hr>
    <br>
    {{#with gallery}}
        <h2>{{title}}</h2>
        <br>
        <div class="row">
            {{#each cards}}
                <div class="col-4 g-2">
                    <div class="card">
                        <div class="card-header">
                            {{name}}
                        </div>
                        <ul class="list-group list-group-flush">
                            {{#each items}}
                                <li class="list-group-item">
                                    {{this}}
                                </li>
                            {{/each}}
                        </ul>
                    </div>
                </div>
            {{/each}}
        </div>
    {{/with}}
    <hr>
    <br>
    {{#with relaxion}}
        <div class="display-3">{{title}}</div>
        <br>
        <div class="row">
            {{#each cards}}
                <div class="col-6 g-2">
                    <div class="card text-bg-dark">
                        <div class="card-header">
                            {{header}}
                        </div>
                        <div class="card-body">
                            {{description}}
                        </div>
                    </div>
                </div>
            {{/each}}
        </div>
    {{/with}}
    <hr>
    <br>
    {{#with rest}}
        <div class="row">
            {{#each cards}}
                <div class="col-6 g-2">
                    <div class="card text-center">
                        <div class="card-header">
                            {{header}}
                        </div>
                        <div class="card-body ">
                            <h5 class="card-title">
                                {{title}}
                            </h5>
                            <p class="card-text">{{description}}</p>
                            <a href="#" class="btn btn-primary">{{button}}</a>
                        </div>
                        <div class="card-footer">
                            {{footer}}
                        </div>
                    </div>
                </div>
            {{/each}}
        </div>
    {{/with}}
    <hr>
    <br>
    {{#with proposition}}
        <div class="display-2">
            {{title}}
        </div>
        <br>
        <div class="row">
            {{#each card}}
                <div class="col-6 g-2">
                    <div class="card text-bg-warning">
                        <div class="card-header">
                            {{header}}
                        </div>
                        <div class="card-body">
                            <h5 class="card-title">
                                {{title}}
                            </h5>
                            <p class="card-text">{{description}}</p>
                        </div>
                    </div>
                </div>
            {{/each}}
        </div>
    {{/with}}
    <hr>
    <br>
    {{#with footer}}
        <h5>{{title}}</h5>
        <br>
        <div class="row">
            <div class="col-3">
                <ul>
                    {{#each navigation}}
                            <li><a href="{{href}}" class="btn btn-link">{{text}}</a></li>
                    {{/each}}
                </ul>
            </div>
            <div class="col-3">
                <ul>
                    {{#each socialMedia}}
                        <div class="col-3">
                            <li><a href="link" class="btn btn-outline-dark">{{name}}</a></li>
                        </div>
                    {{/each}}
                </ul>
            </div>
            <div class="col-3">
                <ul>
                    {{#each partners}}
                        <div class="col-3">
                            <li><a href="{{link}}" class="btn btn-outline-info">{{name}}</a></li>
                        </div>
                    {{/each}}
                </ul>
            </div>
        </div>
    {{/with}}
</main>