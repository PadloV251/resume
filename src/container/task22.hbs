<main class="p-5">
    <section>
        {{#with page_header}}
        <hr>
            <h1 class="text-center">{{title}}</h1>
            <br>
            <p class="text-center text-uppercase">{{description}}</p>
            <hr>
        {{/with}}
        {{#with section}}
            {{#with about_us}}
                <div class="text-end">
                    {{title_list.main}}
                </div>
                <h2 class="text-end">{{title_list.sub}}</h2>
                <br>
                <button class="btn btn-outline-info">{{button.text}}</button>
                <hr>
            {{/with}}
            <br><br>
            {{#with advantage}}
                <div class="display-5 text-muted">{{title}}</div>
                <br>
                {{#with efficiency}}
                    <div class="display-4 text-lowercase text-muted">{{title}}</div>
                    <br>
                    <h6>{{sub_title}}</h6>
                    <br>
                    <p>{{description}}</p>
                    <br>
                    <button class="btn btn-outline-warning">{{button}}</button>
                    <hr>
                {{/with}}
                <br><br>
                {{#with innovation}}
                    <div class="display-4 text-muted text-center">{{title}}</div>
                    <br>
                    <h6 class="text-center">{{sub_title}}</h6>
                    <br>
                    <p class="text-center">{{description}}</p>
                    <br>
                    <button class="btn btn-outline-warning">{{button}}</button>
                    <hr>
                {{/with}}
                <br><br>
                {{#with collaboration}}
                    <div class="display-4 text-muted text-end">{{title}}</div>
                    <br>
                    <h6 class="text-end">{{sub_title}}</h6>
                    <br>
                    <p class="text-end">{{description}}</p>
                    <br>
                    <button class="btn btn-outline-warning">{{button}}</button>
                    <hr>
                {{/with}}
            {{/with}}
            <br><br>
            {{#with agreement}}
                <div class="display-5">{{title}}</div>
                <hr>
                {{#with info}}
                    <h5>{{sub_title}}</h5>
                    <br>
                    {{#with detail}}
                        <p>{{text}}</p>
                        <br>
                        <button class="btn btn-success">{{button}}</button>
                        <hr>
                    {{/with}}
                {{/with}}
            {{/with}}
        {{/with}}
    </section>
</main>